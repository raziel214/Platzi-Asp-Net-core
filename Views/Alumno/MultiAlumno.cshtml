@model List<Alumno>
@{
    ViewData["Title"] = " Informacion Alumno ";
    Layout = "Simple";
    
}

<div class="text-center">
   <h1>@ViewData["Title"]</h1>
  <strong>Desde la vista normal</strong>    
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
  

    @* <strong>Desde la vista parcial</strong> *@
     

@* @foreach(var alumno in Model)
{
    <partial name="ObjetoEscuelaSimple" model="alumno"/>
} *@

<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var alumno in Model)
    {
        <tr>
            <td>@alumno.Nombre</td>
            <td>@alumno.UniqueId</td>
        </tr>
        @* <partial name="ObjetoEscuelaSimple" model="asignatura"/> *@
    }
    </tbody>

</table>

    <p><i>@ViewBag.cosadinamica</i></p>
    <p><i>@ViewData["cosadinamica"]</i></p>
    <p><i>@ViewData["Fecha"]</i></p>
</div>
