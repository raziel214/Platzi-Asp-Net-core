<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var asignatura in Model)
    {
        <tr>
            <td>@asignatura.Nombre</td>
            <td>@asignatura.Id</td>
        </tr>
        @* <partial name="ObjetoEscuelaSimple" model="asignatura"/> *@
    }
    </tbody>

</table>


    <p><i>@ViewBag.cosadinamica</i></p>
