@model List<Asignatura>
@{
    ViewData["Title"] = " Informacion Asignatura ";
    Layout = "Simple";
    
}

<div class="text-center">
   <h1>@ViewData["Title"]</h1>
  <strong>Desde la vista normal</strong>    
